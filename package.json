{
  "name": "whisperide-next",
  "version": "0.1.0",
  "main": "dist/electron/main.js",
  "scripts": {
    "postinstall": "node scripts/install.js",
    "dev": "concurrently \"npm run dev:webpack\" \"npm run dev:electron\"",
    "dev:webpack": "webpack serve --mode development",
    "dev:electron": "tsc -p electron/tsconfig.json && wait-on tcp:8080 && cross-env NODE_ENV=development electron .",
    "start": "electron .",
    "build": "node scripts/build.js",
    "build:react": "webpack --mode production",
    "build:electron": "tsc -p electron/tsconfig.json"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-loader": "^9.1.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^25.0.0",
    "electron-builder": "^24.6.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  }
}